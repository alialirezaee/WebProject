@model IEnumerable<webbook.Models.bookType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.bookTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.book.bookName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.bookTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.book.bookName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.bookTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.bookTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.bookTypeID })
        </td>
    </tr>
}

</table>
